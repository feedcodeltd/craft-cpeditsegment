{
    "name": "tinydots/craft-cpeditsegment",
    "description": "Quickly go to an element's control panel edit URL by adding a segment to the end of its URL on the front-end.",
    "type": "craft-plugin",
    "version": "1.0.0",
    "keywords": [
        "craft",
        "cms",
        "craftcms",
        "craft-plugin"
    ],
    "support": {
        "docs": "https://github.com/tinydots/craft-cpeditsegment",
        "issues": "https://github.com/tinydots/craft-cpeditsegment/issues"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Mike Pepper",
            "email": "mdcpepper@gmail.com",
            "homepage": "https://github.com/mdcpepper"
        }
    ],
    "require": {
        "craftcms/cms": "^3.0.0"
    },
    "require-dev": {
        "codeception/codeception": "2.4.x-dev"
    },
    "autoload": {
        "psr-4": {
            "tinydots\\cpeditsegment\\": "src/"
        }
    },
    "extra": {
        "name": "CP Edit Segment",
        "handle": "cpeditsegment",
        "developer": "Tiny Dots",
        "developerUrl": "https://www.github.com/tinydots",
        "changelogUrl": "https://raw.githubusercontent.com/tinydots/craft-cpeditsegment/master/CHANGELOG.md",
        "class": "tinydots\\cpeditsegment\\CpEditSegment"
    }
}